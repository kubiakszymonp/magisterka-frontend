{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Process Articles",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/tsx",
            "runtimeArgs": ["--env-file=.env"],
            "program": "${workspaceFolder}/scripts/process-articles.ts",
            "args": ["--place=grodzisko_na_zawodziu"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "skipFiles": ["<node_internals>/**", "node_modules/**"]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Debug Process Articles (All)",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/tsx",
            "runtimeArgs": ["--env-file=.env"],
            "program": "${workspaceFolder}/scripts/process-articles.ts",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "skipFiles": ["<node_internals>/**", "node_modules/**"]
        }
    ]
}
